{
  "name": "blog-express-js-app",
  "version": "1.0.0",
  "description": "a blog application backend  in express js",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "server": "nodemon -r dotenv/config --experimental-json-modules backend/src/index.js",
    "client": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\""
